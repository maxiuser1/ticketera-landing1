{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route":"autorizar/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "turno",
      "databaseName": "quehaydb",
      "collectionName": "turnos",
      "connectionStringSetting": "CosmosDBConnection",
      "Id":"{id}",
      "PartitionKey":"quehay"
    }
  ],
  "scriptFile": "../dist/authorizeTrx/index.js"
}